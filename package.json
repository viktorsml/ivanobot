{
  "name": "ivanobot",
  "version": "2.0.0",
  "description": "",
  "main": "dist/bot.js",
  "scripts": {
    "start": "npm run start:bot",
    "start:bot": "node dist/bot.js",
    "start:daemon": "node dist/daemon.js",
    "build": "webpack --mode=production",
    "dev:bot": "npm-run-all --parallel watch:build watch:bot",
    "dev:bot:as-production": "cross-env NODE_ENV=production npm-run-all --parallel watch:build:as-production watch:bot",
    "dev:daemon": "npm-run-all --parallel watch:build watch:daemon",
    "watch:build": "webpack --mode=development --watch",
    "watch:build:as-production": "webpack --mode=production --watch",
    "watch:bot": "nodemon \"./dist/bot.js\" --watch \"./dist/bot.js\"",
    "watch:daemon": "nodemon \"./dist/daemon.js\" --watch \"./dist/daemon.js\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "crypto-js": "^4.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "seedrandom": "^3.0.5",
    "ssh-exec": "^2.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.47",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.13",
    "@types/seedrandom": "^2.4.28",
    "@types/ws": "^7.2.5",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
